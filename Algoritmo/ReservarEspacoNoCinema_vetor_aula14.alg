algoritmo "ReservarEspaçoNoCinema"
// vetor
// exercicio 3
var
 cad: caractere
procedimento Cadeiras()
var
 i: inteiro
inicio
para i <- 1 ate 10 faca
 Escreva("[B", i, "] ")
FimPara

FimProcedimento

inicio

  Cadeiras()
 Escreval(" ")
 Escreval("-------------------------------------------------------------")
 Escreva("Reservar a cadeira: ")
 leia(cad)
 Escreva("A cadeira ", cad, " foi RESERVADA!")
 Escreva("Quer reservar outra (S/N)? ")
 
 
 
fimalgoritmo
